package  {	import flash.display.MovieClip;	import flash.events.Event;		public class Completed extends MovieClip 	{		public var main:Main;				public function Completed(main:Main) 		{			this.main = main;						addEventListener(Event.ADDED_TO_STAGE, addedToStage);		}				public function addedToStage(event:Event)		{			x = stage.stageWidth / 2;			y = stage.stageHeight / 2;		}	}	}