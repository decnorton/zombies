package  {	import flash.display.MovieClip;	import com.greensock.TweenMax;		public class Item extends MovieClip 	{		public var main:Main;		public var pickedUp:Boolean = false;				public var healthValue = 10;		public function Item(main:Main, x:Number, y:Number)		{			this.main = main;			this.x = x;			this.y = y;		}				public function hit()		{			main.nom[Math.round(Math.random() * 3)].play();			pickedUp = true;			TweenMax.fromTo(this, 0.3, { alpha:1 }, { alpha:0, onComplete: hideItem });		}				public function hideItem()		{			this.visible = false;		}	}	}