package  {	import com.greensock.TweenMax;	public class Sandwich extends Item 	{		public function Sandwich(main:Main, x:Number, y:Number) 		{			super(main, x, y);			this.healthValue = 20;		}				override public function hit()		{			var rand = Math.round(Math.random() * 2);			main.nom[rand].play();						this.pickedUp = true;			TweenMax.fromTo(this, 0.3, { alpha:1 }, { alpha:0, onComplete: hideItem });			main.sandwichCount++;		}	}	}